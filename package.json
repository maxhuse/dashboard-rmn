{
  "name": "dashboard-rmn",
  "version": "1.0.0",
  "description": "Dashboard built with React/MobX",
  "main": "server/index.js",
  "scripts": {
    "init": "node sql/create-tables.js",
    "init_dump": "sqlite3 sql/_dashboard.sqlite < sql/dump.sql",
    "start": "NODE_ENV=development nodemon --watch server server/index.js",
    "watch": "npm run clean && NODE_ENV=development webpack --watch --progress",
    "build:production": "npm run clean && NODE_ENV=production webpack --progress",
    "clean": "rimraf public",
    "lint:client": "eslint client --ext .jsx --ext .js",
    "lint:server": "eslint server",
    "lint": "npm run lint:client && npm run lint:server",
    "hot": "npm run clean && NODE_ENV=hot node server/index.js"
  },
  "author": {
    "name": "Max Khudolozhkin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mchu42/dashboard-rmn.git"
  },
  "engines": {
    "node": "8.9.3"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "8.1.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-runtime": "6.26.0",
    "babel-polyfill": "6.26.0",
    "bluebird": "3.5.0",
    "classnames": "2.2.5",
    "css-loader": "1.0.0",
    "es6-promise": "4.2.4",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "mobx": "4.1.0",
    "mobx-react": "5.0.0",
    "mobx-utils": "4.0.0",
    "mobx-react-devtools": "5.0.1",
    "node-sass": "4.9.3",
    "nodemon": "1.18.4",
    "postcss-loader": "2.1.1",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "4.0.0",
    "react-router-dom": "4.2.2",
    "resolve-url-loader": "2.3.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.3",
    "url-loader": "1.0.1",
    "webpack": "3.12.0",
    "webpack-dev-middleware": "2.0.6",
    "webpack-hot-middleware": "2.21.0"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "co": "4.6.0",
    "express": "4.16.3",
    "helmet": "3.12.0",
    "i18next": "10.5.1",
    "lodash": "4.17.5",
    "moment": "2.21.0",
    "serve-favicon": "2.4.5",
